import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int countPositives = 0;
        double sumPositives = 0.0;

        for (int i = 0; i < 6; i++) {
            double value = sc.nextDouble();
            if (value > 0) {
                countPositives++;
                sumPositives += value;
            }
        }

        System.out.printf("%d valores positivos%n", countPositives);
        System.out.printf("%.1f%n", sumPositives / countPositives);

        sc.close();
    }
}
