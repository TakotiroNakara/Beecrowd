import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int positiveCount = 0;
        for (int i = 0; i < 6; i++) {
            double number = sc.nextDouble();
            if (number > 0) {
                positiveCount++;
            }
        }

        System.out.printf("%d valores positivos\n", positiveCount);
    }
}
